<autoexec>
  <!-- Town Gear-->
  <register event="zone_Port_Jeuno">wait 10;sc set town;wait 1;ll profile none</register>
  <!-- Auto join invites -->
  <register event="invite_*">input /join;wait 15;input /p Thanks for The Invite.</register>
  <!-- Plugins -->
  <register silent="true" event="login">drawdistance set 10</register>
  <!-- Light Luggage -->
  <register silent="true" event="Zone_Abyssea*">ll profile Tetas_Abyssea;wait 7;input /echo Tetas's Abyssea Profile Loaded</register>
  <register silent="true" event="Zone_Dynamis*">ll profile Tetas_Dynamis;wait 7;input /echo Tetas's Dynamis Profile Loaded</register>
  <register silent="true" event="Zone_Nyzul*">ll profile Tetas_Nyzul;wait 7;input /echo Tetas's Nyzul Profile Loaded</register>
  <!-- Spellcast -->
  <register silent="true" id="Day|DayWS" event="time_7.*|time_8.*|time_10.*|time_12.*|time_14.*|time_16.*|">sc var set NightOrDay Day</register>
  <register silent="true" id="Night|NightWS" event="time_17.*|time_18.*|time_19.*|time_20.*|time_21.*|time_22.*|time_23.*|time_0.*|time_1.*|time_2.*|time_3.*|time_4.*|time_5.*|time_6.*">sc var set NightOrDay Night</register>
  <!-- Job Related-->
  <!-- Job Changes -->
  <!-- WHM -->
  <register silent="true" event="jobchange_whm/*">input /macro book 1</register>
  <!-- BLM -->
  <register silent="true" event="jobchange_blm/*">input /macro book 2</register>
  <!-- RDM -->
  <register silent="true" event="jobchange_rdm/*">input /macro book 3</register>
  <!-- SCH -->
  <register silent="true" event="jobchange_sch/*">input /macro book 4</register>
